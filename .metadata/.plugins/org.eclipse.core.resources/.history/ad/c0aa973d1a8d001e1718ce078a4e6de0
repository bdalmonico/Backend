package poo;

public class CuentaTest {

	public static void main(String[] args) {
		Titular t  = new Titular();
		t.setEdad(1);
		t.setNombre("asd");
		t.setApelido("ddd");
		
		
		CuentaCorriente cuentaCorriente = new CuentaCorriente(t, "sdfm", 44);
		cuentaCorriente.setNombre("Bruno");
		cuentaCorriente.setApelido("Dalmonico Schossland");
		cuentaCorriente.setEdad(26);
		
		cuentaCorriente.imprimirTitular();
		
		System.out.println(cuentaCorriente.getApelido());
		
		
		
	}

}
